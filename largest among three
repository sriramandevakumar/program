import java.util.*;
class Largest
{
public static void main(String args[])
{ int a, b, c;
System.out.println("Enter three integers ");
Scanner l = new Scanner(System.in);
a= l.nextInt();
b= l.nextInt();
c= l.nextInt();
if ( a>b && a>c )
System.out.println(a+" is largest.");
else if ( b>c&& b>a )
System.out.println(b+" is largest.");
else if ( c>a && c>b )
System.out.println(c+" is largest.");
}
}
